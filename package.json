{
  "name": "waviot-data-transfer",
  "version": "1.0.0",
  "description": "Перенос веток",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=dev concurrently -n=svelte,electron -c='#ff3e00',blue \"npm run dev:svelte\" \"npm run dev:electron\"",
    "dev:svelte": "svelte-kit dev",
    "dev:electron": "electron src/electron.cjs",
    "build": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron",
    "build:svelte": "svelte-kit build",
    "build:electron": "electron-builder -mwl --config build.config.json",
    "start": "svelte-kit start"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.19",
    "@sveltejs/kit": "^1.0.0-next.168",
    "cross-env": "^7.0.3",
    "electron": "^14.0.1",
    "electron-builder": "^22.11.7",
    "electron-connect": "^0.6.3",
    "electron-packager": "^15.4.0",
    "electron-reloader": "^1.2.1",
    "electron-updater": "^4.3.9",
    "svelte": "^3.42.6",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "vite": "^2.5.8"
  },
  "dependencies": {
    "concurrently": "^6.2.1",
    "electron-context-menu": "^3.1.1",
    "electron-serve": "^1.1.0",
    "electron-window-state": "^5.0.3",
    "express": "^4.17.1"
  }
}
